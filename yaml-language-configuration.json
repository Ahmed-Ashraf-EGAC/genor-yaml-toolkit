{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "'",
            "close": "'"
        }
    ],
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*.*:\\s*([^\\{\\[]*[\\{\\[].*)?$",
        "decreaseIndentPattern": "^\\s*[}\\]],?\\s*$"
    },
    "folding": {
        "markers": {
            "start": "^\\s*#\\s*region\\b",
            "end": "^\\s*#\\s*endregion\\b"
        }
    },
    "wordPattern": "[a-zA-Z_][a-zA-Z0-9_-]*"
}